	<target name="<%=new GenerationHelper().getIdOrPositionString(egfStep)%>" depends="init,install.egf">
		<egf>
            <globargs>
<%for(Property property : egfStep.getProperties()) {%>
				<jvmarg value="-D<%=property.getKey()%>=<%=property.getValue()%>" />
<%}%>            
			</globargs>
			<activities>
<%for(EgfActivity egfActivity: egfStep.getEgfActivities()) {%>
				<arg value="<%=egfActivity.getUri()%>"/>
<%}%>
			</activities>
		</egf>
	</target>
