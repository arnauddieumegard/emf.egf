<plugin>
   <extension-point id="elements" name="%extension-point.name" schema="schema/elements.exsd"/>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.eclipse.org/egf/1.0.0/generationChain"
            class="org.eclipse.egf.portfolio.genchain.generationChain.GenerationChainPackage"
            genModel="model/generationChain.genmodel"/>
   </extension>

	<extension point="org.eclipse.egf.core.fcore">
	    <fcore id="egf/generationChain.fcore"/>
	</extension>
 
 	<extension point="org.eclipse.egf.portfolio.genchain.elements">
    	<helper id="basic.emf.generation" class="org.eclipse.egf.portfolio.genchain.extension.DefaultEmfGenerationExtension"/>
    	<helper id="basic.documentation.generation" class="org.eclipse.egf.portfolio.genchain.extension.DefaultEmfDocumentationExtension"/>
    </extension>

</plugin>
