<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="productionplan"
    nsURI="http://www.eclipse.org/egf/1.0.0/productionplan" nsPrefix="productionplan">
  <eClassifiers xsi:type="ecore:EClass" name="ProductionPlan" eSuperTypes="FactoryComponent.ecore#//Orchestration">
    <eStructuralFeatures xsi:type="ecore:EReference" name="productionPlanInvocations"
        upperBound="-1" eType="#//ProductionPlanInvocation" containment="true" resolveProxies="false"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ProductionPlanInvocation" abstract="true"
      eSuperTypes="FactoryComponent.ecore#//Invocation"/>
  <eClassifiers xsi:type="ecore:EClass" name="FactoryComponentInvocation" eSuperTypes="#//ProductionPlanInvocation">
    <eStructuralFeatures xsi:type="ecore:EReference" name="factoryComponent" lowerBound="1"
        eType="ecore:EClass FactoryComponent.ecore#//FactoryComponent"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TaskInvocation" eSuperTypes="#//ProductionPlanInvocation">
    <eStructuralFeatures xsi:type="ecore:EReference" name="task" lowerBound="1" eType="ecore:EClass FactoryComponent.ecore#//Task"/>
  </eClassifiers>
</ecore:EPackage>
